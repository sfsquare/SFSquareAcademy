@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BATCH.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CLASS_TIMING.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMPLOYEE.EMP_NUM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SUBJECT.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TIMETABLE.IS_ACT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WEEKDAY.NAME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
