@model IEnumerable<SFSAcademy.TIMETABLE_ENTRY>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BATCH.NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CLASS_TIMING.NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMPLOYEE.EMP_NUM)
        </th>
