
@{
    IEnumerable<SFSAcademy.MonthyPayslip> dates = (IEnumerable<SFSAcademy.MonthyPayslip>)ViewData["dates"];
}

@if (dates != null && dates.Count() != 0)
{
    <div id="submit-button" class="approve">
        @Html.ActionLink("► Approve", "One_Click_Approve_Submit", new { date = ViewBag.salary_date })
    </div>
}
else
{
    <div id="submit-button" class="approved">
        Already Approved.
    </div>
}
