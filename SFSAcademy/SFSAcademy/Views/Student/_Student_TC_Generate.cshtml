@model SFSAcademy.STUDENT
@{
    IEnumerable<SFSAcademy.StundentFee> paid_fees = (IEnumerable<SFSAcademy.StundentFee>)ViewData["paid_fees"];
}
@if (ViewBag.ErrorMessage != null)
{
    <p class="flash-msg"> @ViewBag.ErrorMessage </p>
}
else
{
    <h4>Student Deleted Successfully</h4>
    <br />
    <br />
    @Html.ActionLink("Generate TC", "Generate_TC_pdf", new { Adm_No = @ViewBag.ADMSN_NO, Status_Descrition = @ViewBag.Status_Descrition }, new { @target = "_blank", @class = "user_button", @id = "admission_button", @title = "Generate TC" })

}

