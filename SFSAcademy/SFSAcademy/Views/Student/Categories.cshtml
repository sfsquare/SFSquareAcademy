@model IEnumerable<SFSAcademy.STUDENT_CATGEORY>
@{
    ViewBag.Title = "Categories";
}

<div id="content-header">
    <img src="~/Content/images/show_settings.png" alt="Settings Logo">
    <h1>Students</h1>
    <h3>Categories</h3>
    <div id="app-back-button">
        <a href='javascript:history.back()' onclick="return confirm('are you sure?')"><img src="~/Content/images/buttons/back.png"></a>
    </div>
</div>
<div><img src="~/Content/images/_global/header_underline_.jpg" width="100%"></div>

<div id="page-yield">
    <div id="student-category-form">
        @Html.Partial("~/Views/Student/_student_category_create_form.cshtml", new SFSAcademy.STUDENT_CATGEORY())
    </div>
    <br /><br />
    <div id="student-category-list">
        @{Html.RenderAction("_CategoriesList", "Student");}

    </div>
</div>
