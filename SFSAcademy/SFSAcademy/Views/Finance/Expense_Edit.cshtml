@model SFSAcademy.FINANCE_TRANSACTION
@{
    ViewBag.Title = "Expense_Edit";
}
<div id="content-header">
    <img src="~/Content/images/show_finance.png" alt="Finance Logo">
    <h1>Expense</h1>
    <h3>Edit Expense</h3>
    <div id="app-back-button">
        <a href='javascript:history.back()' onclick="return confirm('are you sure?')"><img src="~/Content/images/buttons/back.png"></a>
    </div>
</div>
<div><img src="~/Content/images/_global/header_underline_.jpg" width="100%"></div>
<div id="inner-tab-menu">
    <ul>
        <li>@Html.ActionLink("Expenses", "Expense_List")</li>
    </ul>
</div>
<div id="page-yield">
    @if (ViewBag.ErrorMessage != null)
    {
        <p class="flash-msg"> @ViewBag.ErrorMessage </p>
    }
    <div class="box">
        @Html.Partial("~/Views/Finance/_Transaction_Edit_Form.cshtml", Model, new ViewDataDictionary { { "PageName", "Expense_Edit" }, { "CAT_ID", @ViewData["CAT_ID"] }, { "SpecialCategory", @ViewData["SpecialCategory"] } })
    </div>
</div>

